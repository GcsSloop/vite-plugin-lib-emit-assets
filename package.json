{
  "name": "vite-plugin-lib-emit-assets",
  "type": "module",
  "version": "1.0.0",
  "description": "Vite 插件：提取 lib 构建模式下引用到的资源文件，取代 base64 内联形式",
  "scripts": {
    "build": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "dev": "tsup --watch src",
    "publish": "npm publish",
    "release": "bumpp && npm publish"
  },
  "license": "MIT",
  "homepage": "https://github.com/GcsSloop/vite-plugin-lib-emit-assets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GcsSloop/vite-plugin-lib-emit-assets.git"
  },
  "bugs": {
    "url": "https://github.com/GcsSloop/vite-plugin-lib-emit-assets/issues"
  },
  "keywords": [
    "vite-plugin",
    "vite",
    "emit lib assets"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "loader-utils": "^3.2.1",
    "vue": "^3.2.33",
    "mrmime": "^1.0.1",
    "vite": "^4.4.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/loader-utils": "^2.0.3",
    "@types/node": "^18.16.16",
    "bumpp": "^9.1.0",
    "chalk": "^5.2.0",
    "eslint": "^8.42.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "rollup": "^4.1.4",
    "vitest": "^0.31.4"
  },
  "volta": {
    "node": "18.16.1"
  }
}
